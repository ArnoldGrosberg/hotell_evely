<body style="text-align: center;">
  <!-- Main header -->
  <header class="flexbox-container form">

    <!-- Logo (text+image) -->
    <a href="/">
      <div class="logo flexbox-container">

        <div class="name">
          <span class="company_name">Teenuse nimi</span>
        </div>
        <div class="img">
          <img class="company_img" draggable="false" src="../images/logo.jpg" alt="logo">
        </div>
        
      </div>
    </a>

    <!-- Navbar area -->
    <div class="navbar">

      <!-- POLE VAJA! <a class="left" href="lisa_majutus">Lisa oma majutusasutus</a> -->
      <!-- POLE VAJA!  <a class="center" href="login">Logi sisse</a> -->

    </div>

  </header>
    <div style=" display: inline-block; background-color: rgba(68, 119, 251, 1); width: 50%; height: 100%;">
      <form style=" margin-top: 40px; color: white;" action="/lisa_majutus" method="post">
        <title>Majutus</title>
        <h1>Majutus</h1>
        <div style="text-align: center;">
          <label style="display: inline-block;" for="Asutus" required>Asutuse nimi</label><br>
          <input type="text" class="form-control-lg" style="width: 350px" id="Asutus" name="Asutus"
            placeholder="Asutuse nimi">
        </div><br>
        <div style="text-align: center;">
            <label for="Kirjeldus" required>Kirjeldus</label><br>
            <textarea class="form-control-lg" id="Kirjeldus" rows="3" style="width: 350px"
              placeholder="Kirjeldus" name="Kirjeldus"></textarea>
        </div><br>
        <div style="text-align: center;">
          <label style="display: inline-block;" for="Asukoht" required>Asukoht</label><br>
          <input type="text" class="form-control-lg" style="width: 350px" id="Asukoht"
            placeholder="Asutuse nimi">
        </div><br>
        <div style="text-align: center;">
          <label style="display: inline-block;" for="Tärni" required>Mitu tärni on see hotell?</label><br>
            <input type="range" min="0" max="5" value="2.5" class="slider" id="Tärni" style="width: 350px;" name="Tärni">
        </div><br>
        <div style="text-align: center;">
          <label style="display: inline-block;" for="ruumi" required>Mitu ruumi on hotellis?</label><br>
          <input type="text" class="form-control-lg" style="width: 350px" id="ruumi"
            placeholder="Mitu ruumi on hotellis?">
        </div><br>
        <div style="text-align: center;">
          <label style="display: inline-block;" for="arv" required>Ööbimiskohtade arv</label><br>
          <input type="text" class="form-control-lg" style="width: 350px" id="arv"
            placeholder="Ööbimiskohtade arv">
        </div><br>
        <div style="text-align: center;">
          <label style="display: inline-block;" for="lähedasi" required>Lisa lähedasi teenuseid, kui on</label><br>
          <input type="text" class="form-control-lg" style="width: 350px" id="lähedasi"
            placeholder="Lisa lähedasi teenuseid, kui on">
        </div><br>
        <div style="text-align: center;">
          <label style="display: inline-block;" for="teenuseid" required>Lisa hotelli teenuseid, kui on</label><br>
          <input type="text" class="form-control-lg" style="width: 350px" id="teenuseid"
            placeholder="Lisa hotelli teenuseid, kui on">
        </div><br>
        <div style="text-align: center;">
          <label style="display: inline-block;" for="pilt" required>Lisa pilt hotellist</label><br>
            <input class="form-control form-control-lg" id="pilt" type="file">
        </div><br>
        <div style="text-align: center;">
          <button type="submit" class="btn btn-primary" style="background-color: white; text-align: center; width: 350px; font-size: 25px; color: black;">Lisa majutusasutus</button>
        </div>
    </div>
  </div>
  
  </form>
  </div>
</body>
</html> <br /><br />